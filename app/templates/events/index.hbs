<div class="ui segments">
  {{#each model as |event|}}
    <div class="ui segment">
      <div class="ui grid margin-no">
        <div class="two wide column padding-no text-center" style="max-width: 56px">
          <div class="ui segments border-none box-shadow-none">
            <div class="ui segment text-size-big text-weight-bold padding-no-left padding-no-right padding-md-top padding-vs-bottom">
              {{moment-format event.date "DD"}}
            </div>
            <div class="ui segment padding-vs border-none">
              {{moment-format event.date "MMM"}}
            </div>
          </div>
        </div>
        <div class="six wide column">
          
        </div>
      </div>
    </div>
  {{/each}}

</div>
{{outlet}}